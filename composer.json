{
    "name": "webapps/util",
    "description": "Utility Library for Modern PHP",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Justin Scott",
            "email": "justin@hitpath.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "dms/phpunit-arraysubset-asserts": "^0.2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webapps\\Util\\Providers\\UtilServiceProvider"
            ]
        }
    },
    "autoload": {
        "files": ["src/helpers.php"],
        "psr-4": {
            "Webapps\\Util\\": "src/"
        }
    }
}
